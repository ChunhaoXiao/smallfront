.page{
  width: 750rpx;
  height: 95rpx;
}
.nav-header{
  position:fixed;
  z-index:99;
  width:100%;
  height: 100rpx;
  background-color: #f9c00d;
  padding-top: 17rpx;
  box-sizing: border-box;
}
.page__bd{
  margin-top: 50px
}
.shaoyishao{
  display: inline-block;
  background: pink;
   height: 30px;
   width:30px
}
.search view{
  display: inline-block;
}
.input_box{
  width: 558rpx;
  height: 60rpx;
  margin: auto;
  margin-left: 42rpx;
  background: #fce086;
}
.search_icon{
  margin-top: 19rpx;
  margin-left: 29rpx;
  float: left
}
.search .search-input{
  display: inline-block;
  background-color:#fce086;
  margin-left: 10rpx;
  height: 60rpx;
  width: 430rpx;
  line-height: 60rpx;
}
.i_tishi{
  font-size: 26rpx;
  color: #191515;
}
.search-input icon{
  position:absolute;
  left:5px;
  top:10px;
}
.outside{
  position: absolute
}
.search-select{
  display: inline-block
}
.local{
  display: inline-block;
  border: 1px #ac8945 solid;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 28rpx;
  text-align: center;
  padding: 0rpx 20rpx 0rpx 20rpx;
  margin-top: 43rpx;
  margin-left: 30rpx;
}
.clearInput{
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  float: right;
  margin-right: 19rpx;
  margin-top:15rpx; 
  background: #f9c00d;
  color: #fff;
  padding: 0rpx;
  line-height: 30rpx;
  font-size: 16rpx;
  border: 0rpx;
  opacity: 0.6;
}
.search_button{
  width: 112rpx;
  height: 60rpx;
  float: right;
  margin-right: 36rpx;
  text-align: center;
  line-height: 60rpx;
  font-size: 26rpx;
  background: #fce086;
  color: #f9c00d;
  border-radius: 0rpx;
  border: 0rpx;
  padding: 0rpx;
  position: static;
}
.zong{
  width: 677rpx;
  height: 100%;
  padding-top: 118rpx; 
  margin: auto;
}
.zong_text{
  height: 27rpx;
  width: 600rpx;
  float: left;
  line-height: 27rpx;
  font-size: 28rpx;
}
.modal{
  width: 262rpx;
  height: 140rpx;
}
.sc_button{
  width: 26rpx;
  height: 30rpx;
  float: right;
  position: static;
  border: 0rpx;
  border-radius: 0rpx;
  padding: 0rpx;
  background: none;
  line-height: 30rpx;
}
.sc_button image{
  width: 26rpx;
  height: 30rpx;
  display: block;
}
