.page {
  width: 100%;
  height: 100%;
  padding-top: 34rpx;
  box-sizing: border-box;
}

.refund1 {
  width: 686rpx;
  height: 420rpx;
  margin: auto;
  background: #fff;
  border: 1rpx #eee solid;
  box-sizing: border-box;
}

.refund1_box {
  width: 640rpx;
  height: 390rpx;
  margin: auto;
}

.refund1_title {
  width: 100%;
  height: 87rpx;
}

.r_t_left {
  height: 87rpx;
  float: left;
  line-height: 87rpx;
  font-size: 28rpx;
}

.r_t_right {
  height: 87rpx;
  float: right;
  line-height: 87rpx;
  font-size: 28rpx;
  color: #f9c00d;
}

.refund1_line {
  width: 640rpx;
  height: 2rpx;
  background: #eee;
}

.refund1_list {
  width: 640rpx;
  height: 120rpx;
  margin-top: 30rpx;
}

.refund1_list image {
  width: 161rpx;
  height: 120rpx;
  float: left;
}

.refund1_list_you {
  width: 460rpx;
  height: 100%;
  float: right;
}

.refund1_list1 {
  width: 100%;
  height: 28rpx;
  line-height: 28rpx;
  font-size: 28rpx;
  overflow: hidden;
}

.refund1_list2 {
  width: 100%;
  height: 65rpx;
  line-height: 65rpx;
  font-size: 24rpx;
  color: #999;
}

.refund1_list2_you {
  height: 65rpx;
  line-height: 65rpx;
  font-size: 24rpx;
  color: #999;
  float: right;
}

.refund1_list3 {
  width: 100%;
  height: 20rpx;
  font-size: 24rpx;
  line-height: 20rpx;
}

.refund1_Total {
  width: 350rpx;
  height: 72rpx;
  margin-left: 290rpx;
  line-height: 72rpx;
  font-size: 24rpx;
}

.refund1_Total_you {
  height: 72rpx;
  line-height: 72rpx;
  float: right;
  font-size: 30rpx;
}

.refund1_button {
  width: 357rpx;
  height: 64rpx;
  margin-top: 16rpx;
  margin-left: 278rpx;
}

.refund1_b_cs {
  width: 160rpx;
  height: 100%;
  float: left;
  background: #fff;
  border: 1rpx #f9c00d solid;
  color: #f9c00d;
  font-size: 28rpx;
  box-sizing: border-box;
  line-height: 64rpx;
  text-align: center;
}

.refund1_b_content {
  width: 160rpx;
  height: 100%;
  float: right;
  background: #f9c00d;
  color: #fff;
  font-size: 28rpx;
  line-height: 64rpx;
  text-align: center;
}

.cs_zhao {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background: #000;
  z-index: 9000;
  opacity: 0.3;
}
.cs_phone{
  width: 750rpx;
  height: 250rpx;
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  z-index: 9999;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.phone_number{
  width: 680rpx;
  height: 106rpx;
  text-align: center;
  background: #fff;
  margin: auto;
  line-height: 106rpx;
  font-size: 35rpx;
}
.phone_close{
  width: 680rpx;
  height: 106rpx;
  text-align: center;
  background: #fff;
  margin: auto;
  margin-top: 20rpx;
  line-height: 106rpx;
  font-size: 35rpx;
}
